{
    "name": "guillaumeferron/post-content",
    "description": "A post content builder field for Laravel Nova.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Guillaumeferron\\PostContent\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Guillaumeferron\\PostContent\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
